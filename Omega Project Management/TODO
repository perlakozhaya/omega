Employee:	String nameCode
			double workedHours
			Specialty	--->	static MAP < String , double >
								String specialtyName
			
			- cost();	//workedHours * wagePerHour{from the MAP}
			
Item:	static MAP < String , double > 
		String itemName
		double quantity
		
		- cost();	// quantity * pricePerUnit{from the MAP} 
